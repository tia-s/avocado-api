type Query {
    getAllAvocadoSales: [AvocadoSale]
    getAvocadoSaleById(id: ID!): AvocadoSale
    getAvocadoSalesByPriceRange(minPrice: Float!, maxPrice: Float!): [AvocadoSale]
}
