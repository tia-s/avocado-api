type AvocadoSale {
    id: ID!
    date: String!
    averagePrice: Float!
    totalVolume: Float!
    plu4046: Float!
    plu4225: Float!
    plu4770: Float!
    totalBags: Float!
    smallBags: Float!
    largeBags: Float!
    xLargeBags: Float!
    type: String!
    year: Int!
